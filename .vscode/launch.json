	"version": "0.2.0",
	"configurations": [
		{
			"name": "8080 Launch Chrome",
			"request": "launch",
			"type": "chrome",
			"url": "http://localhost:8080",
			"webRoot": "${workspaceFolder}"
		},
		{
			"type": "chrome",
			"request": "launch",
			"name": "5174 Launch Chrome",
			"url": "http://localhost:5174",
			"webRoot": "${workspaceFolder}"
		},
		{
			"type": "chrome",
			"name": "4174 Launch Preview on Chrome",
			"request": "launch",
			"url": "http://localhost:4174",
			"webRoot": "${workspaceFolder}"
		},
		{
			"type": "chrome",
			"name": "4174 Attach Preview on Chrome",
			"request": "attach",
			"url": "http://localhost:4174",
			"webRoot": "${workspaceFolder}"
		},
		{
			"type": "chrome",
			"name": "5175 Attach Chrome",
			"request": "attach",
			"url": "http://localhost:5175",
			"webRoot": "${workspaceFolder}"
		}
	]
}
